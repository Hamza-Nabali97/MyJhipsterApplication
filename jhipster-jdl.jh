entity Company {
	name String required,
    location String required,
}
entity Tile {
	name String required,
    code String required minlength(4),
    size String required,
    placement String required,
    polishType String required,
    costPrice Double required,
    minimumSellingPrice Integer required,
    inventoryQunatity Integer required
}
relationship OneToMany {
Company{tile} to Tile{company}
}

dto all with mapstruct
service all with serviceClass
paginate all with pagination